<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.image.ImageView?>
<fx:root type="StackPane"
         xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml">
    <HBox>
        <Label fx:id="healthLabel"/>
        <Label fx:id="flagsLabel"/>
        <Button fx:id="characterButton">
            <graphic>
                <ImageView fx:id="characterImageView" fitHeight="128" fitWidth="128" preserveRatio="true"/>
            </graphic>
        </Button>
        <Button fx:id="quitButton" text="Quit"/>
    </HBox>
</fx:root>
